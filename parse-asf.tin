(defnet parse-asf (info f)
        (not <info "aborted">)
        (fsetpos 0 f)
        (= (parse-asf-guid f) 0)
        (fsetpos 0 f)

        (set <info "filetype"> "asf")
        (set <info "cleaner"> (netptr parse-asf-clean)) )

(defnet parse-asf-clean (fileinfo)
        (skip) )


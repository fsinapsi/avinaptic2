(defun parse-asf-guid1 (f)
        (parse-asf-guid2 f 16) )

(defun parse-asf-guid2 (f n)
        (if (= n 0) nil
                (parse-asf-guid3 (freadchar f) f n) ))

(defun parse-asf-guid3 (c f n)
        (if (= c undef) undef
                (parse-asf-guid4 c (parse-asf-guid2 f (- n 1))) ))

(defun parse-asf-guid4 (c l)
        (if (= l undef) undef
                (cons (length c) l) ))

